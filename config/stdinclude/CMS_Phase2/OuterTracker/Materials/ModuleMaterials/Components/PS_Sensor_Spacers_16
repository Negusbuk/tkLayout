// Sensor spacers for PS module 1.6 mm spacing

Component {
  componentName "PS Module: Sensor spacers"
  service false
  scaleOnSensor 0
  targetVolume 7

  //1.6mm spacer
  Element {
    elementName AlN
    quantity 1.120   // for 4 spacers: V = 314.0667 mm^3
    unit g
  }

  //Bridges glue 1.6 mm
  Element {
    elementName Epoxy
    quantity 0.163
    unit g
  }
}
