// Sensor spacers for PS module 4.0 mm spacing

Component {
  componentName "PS Module: Sensor spacers"
  service false
  scaleOnSensor 0
  targetVolume 7

  //4.0mm spacer
  Element {
    elementName AlN
    quantity 2.64   // 4 spacers: V = 1256.1842 mm^3
    unit g
  }

  //Bridges glue 4.0 mm
  Element {
    elementName Epoxy
    quantity 0.163
    unit g
  }
}
