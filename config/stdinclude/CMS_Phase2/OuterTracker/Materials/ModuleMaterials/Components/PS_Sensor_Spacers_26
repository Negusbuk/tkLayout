// Sensor spacers for PS module 2.6 mm spacing

Component {
  componentName "PS Module: Sensor spacers"
  service false
  scaleOnSensor 0
  targetVolume 7

  //2.6mm spacer
  Element {
    elementName AlN
    quantity 1.86   // 4 spacers: V = 706.6036 mm^3
    unit g
  }

  //Bridges glue 2.6 mm
  Element {
    elementName Epoxy
    quantity 0.163
    unit g
  }
}
