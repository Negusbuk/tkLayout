// TEXP Ring1: module with 2 chips
// 2.5 modules/GBT 

Conversion {
  Input {
    // Two conductors
    Element {
      elementName Cu_Elinks_TEPX_R1
      quantity 1.6575              // = 2.5 * quantity / module
      unit g/m
    }
  }
  Output {
    @include GBT_and_fibres        // = 1 GBT
  }
}
