// MATERIALS per ROC

// Capacitors for one pixel chip
// According to Murata web page (from Stella)
Component {
  componentName "IT Module: ROC (capacitors)"  // NB: THIS NEEDS TO BE CONFIRMED!!!! ASK STELLA, VALUE COULD BE TOO BIG.
  service false
  scaleOnSensor 0
  Element {
    elementName Pix_Caps
    quantity 0.1878
    unit g
    targetVolume 3
  }
}


Component {
  componentName "IT Module: ROC (Si)"  // Source: [TEPX_2018_12_07_LC]  0.57g for 4 ROCs, hence ~0.1425 g per ROC
  service false
  scaleOnSensor 0
  Element {
    elementName Si
    quantity 0.1425       
    unit g             
    targetVolume 3
  }
}


